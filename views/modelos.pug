doctype html
html(lang="es")
    head
        meta(charset="UTF-8")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Profile
        link(rel="stylesheet", href="css/modelos.css")
    body
        include layout/navbar.pug 
        if data === null
            button(class="move" id="add-model") 
                img(src="icons/mas.svg", alt="mas-icon")
        else
            button(class="move" id="add-model") 
                img(src="icons/mas.svg", alt="mas-icon")
        main(id="mainCont")
            if error
                include layout/error.pug
            else
                if data === null
                    include layout/emptyModel.pug
                else
                    include layout/modelos.pug
        
        script(src="js/jquery.min.js")
        script(src="https://cdn.jsdelivr.net/npm/sweetalert2@9")
        script(src="https://unpkg.com/axios/dist/axios.min.js")
        script(src="js/perfil.js")
        script(src="js/modelos.js")

