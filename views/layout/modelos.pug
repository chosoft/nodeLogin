div(class="wp-modelos")
    each val in data
        div(class="card" idModelo=`${val._id}`)
            div(class="card-header")
                h2= val.nombre
            div(class="card-img")
                img(src="/modelosImg/logo.svg", alt="", srcset="")
            div(class="card-btns")
                a(href=`/modelos/${val.nombre}` class="link-btn") Ir 
                button(class="btn-delete" deleterKey=`${val._id}`) Eliminar